#include <stdio.h>
#include <stdlib.h>
#include <string.h>

main() {
	char ch, flag=-1;
	unsigned c=0, n=0, w=0;
 
	while ((ch = getchar()) != EOF) {
		if (ch == '\n') n++; // строки
		else c++; // символы
 
		if (ch == ' ' || ch == '\n') flag = -1; // слова
		else
			if (flag == -1) {
				flag = 1;
				w++;
			}
	}
 
	printf("%u %u %u\n", c, n, w);
}







